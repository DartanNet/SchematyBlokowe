a=float(input("podaj wartość a"))
b=float(input("podaj wartośc b"))
c=float(input("podaj wartość c"))
if a==0:
    print("to nie jest równanie kwadratowe")

else:
     delta=b**2-4*a*c
     print("twoja delta:")
     print(delta)
     if delta<0:
        print("brak rozwiązań")
     if delta==0:
       x0=(-b) / (2*a)
       print("x0")
       print(x0)
     elif delta>0:
       x1=(-b-(delta**0.5)) / (2*a)
       x2=(-b+(delta**0.5)) / (2*a)

       print("x1",x1,"x2",x2)



